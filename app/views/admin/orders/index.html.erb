<h2>订单管理</h2>

<table class="table table-boldered">
    <thead>
      <tr>
        <th colspan="6">众筹回报</th>
        <th>回报价格</th>
        <th>支持数量</th>
        <th>合计金额</th>
        <th>订单时间</th>
        <th>操作</th>
      </tr>
    </thead>
    <tbody>
      <%= render(partial: "order_item", collection: @orders, as: "item") || render_order_empty_warning %>
    </tbody>
  </table>

  <div class="text-center">
    <%= will_paginate @projects %>
  </div>
